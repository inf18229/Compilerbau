#!/bin/bash

bison -d Praed_Lex.y --warnings=none
flex -t Praed_Lex.l>Praed_Lex.c
gcc -c -o Praed_Lex.o Praed_Lex.c
gcc -c -o Praed_Lex.tab.o Praed_Lex.tab.c
gcc Praed_Lex.o Praed_Lex.tab.o -o Parser
